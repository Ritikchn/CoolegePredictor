<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>College Predictor</title>
    <link rel="icon" href="favicon.ico?v=2">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="css/navbarandabout-us.css">
    <link rel="stylesheet" href="css/table.css">
  </head>
  <body>
 <%-include("partials/navbar")-%>
   <section id="result">
     <div class="heading">
       <h1>Colleges you can get from <%=examname%> exam</h1>

     </div>
   <div class="result-table">
     <table class="table table-striped">
       <!-- <thead> -->
         <tr>

           <th scope="col">College</th>
           <th scope="col">Course</th>
           <th scope="col">Cutoff </th>
         </tr>
       <!-- </thead> -->
       <!-- <tbody> -->
         <%tableData.forEach(function(data){%>
           <tr>
             <td class="result"><%=data.college_name%></td>
             <td class="result"><%=data.course%></td>

             <% switch (role) {
                case 'General' : %>
                      <td class="result"><%=data.General%></td>
                        <% break;

                case 'OBC' : %>
                      <td class="result"><%=data.OBC%></td>
                        <%break;

              case 'SC' : %>
                      <td class="result"><%=data.SC%></td>
                        <%break;

                case 'ST' : %>
                      <td class="result"><%=data.ST%></td>
                      <%break;

                case 'PWD' : %>
                    <td class="result"><%=data.PWD%></td>
                      <%break;
                      default :
                }%>


           </tr>
         <%})%>
       <!-- </tbody> -->
     </table>
   </div>
   </section>

  <%-include("partials/about-us")-%>
  </body>
</html>
