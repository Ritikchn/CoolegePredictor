    <div class="form-container container-fluid">
      <h1>Fill information for GATE EE</h1>
      <form  action="gateEE.table" method="post">
        <div class="row">
          <div class="col-lg-6">
            <div class="form-group">
              <label for="rank">GATE EE Marks</label>
